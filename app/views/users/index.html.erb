<div class="container">
  <div class="row">
    <h3>Users</h3>
    <div class="col-sm-6">
      <table class="table">
        <thead>
          <tr>
            <th>Name</th>
            <th>GitHub Profile</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <td><%= user.name %></td>
              <td><a href="https://github.com/<%= user.username %>"><%= user.username %></td>
              <td><%= link_to "View Profile", user %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
